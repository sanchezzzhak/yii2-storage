{
    "name": "kak/storage",
    "description": "file upload/resize/crop file move storage",
    "type": "yii2-extension",
	"homepage": "https://github.com/sanchezzzhak/yii2storage",
    "keywords": ["yii2","extension","storage"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Tutik Alexsandr",
            "email": "sanchezzzhak@ya.ru"
        }
    ],
	"require": {
		"php":">=5.4.0",
        "aws/aws-sdk-php": "2.*",
		"yiisoft/yii2": "*",
		"yiisoft/yii2-jui": "*",
        "yiisoft/yii2-imagine": "*",
        "bower-asset/blueimp-file-upload": "9.10.5",
        "bower-asset/cropper": "0.10.1"
	},
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
	"autoload": {
		"psr-4": {
			"kak\\storage\\": ""
		}
	}
}